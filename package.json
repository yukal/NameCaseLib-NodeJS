    "russian",
    "ru",
    "language",
    "fio",
    "fullname",
    "name",
    "surname",
    "patronymic"
  ],
  "author": "Andriy Chaika <andriy@chaika.in.ua>",
  "contributors": [
    "Rabotyahoff <https://github.com/Rabotyahoff>",
    "ukrbubik <ukrbubik@gmail.com>",
    "Alex Yu <yukal.alexander@gmail.com>"
  ],
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "cz-conventional-changelog": "^3.3.0",
    "mocha": "^2.4.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yukal/declension-name"
  },
  "dependencies": {
    "locutus": "^2.0.9"
